<?xml version="1.0"?>
<data>
    <xpath expr="/form/notebook" position="inside">
        <page string="Personal Details" id="pd">
            <label name="gender"/>
            <field name="gender" widget="selection"/>
            <label name="date_of_birth"/>
            <field name="date_of_birth"/>
            <label name="place_of_birth"/>
            <field name="place_of_birth"/>
            <label name="height"/>
            <field name="height"/>
            <label name="weight"/>
            <field name="weight"/>
            <label name="blood_group"/>
            <field name="blood_group"/>
            <label name="identification_mark"/>
            <field name="identification_mark"/>
            <label name="pan_card_number"/>
            <field name="pan_card_number"/>
            <label name="marital_status"/>
            <field name="marital_status" widget="selection"/>
            <label name="hobbies"/>
            <field name="hobbies" yexpand="0"/>
        </page>
        <page string="Educational Details" id="educational_details">
            <field name="education"/>
        </page>
    </xpath>
</data>
